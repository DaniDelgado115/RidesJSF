	<managed-bean>
		<managed-bean-name>queryRides</managed-bean-name>
		<managed-bean-class>eredua.bean.QueryRides</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>hasiera</managed-bean-name>
		<managed-bean-class>eredua.bean.Hasiera</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>createRide</managed-bean-name>
		<managed-bean-class>eredua.bean.CreateRide</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>eredua.bean.LoginBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
