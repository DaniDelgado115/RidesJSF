	</managed-bean>
	<managed-bean>
		<managed-bean-name>deleteUser</managed-bean-name>
		<managed-bean-class>eredua.bean.DeleteUser</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>getDriverRides</managed-bean-name>
		<managed-bean-class>eredua.bean.getDriverRides</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>info</managed-bean-name>
		<managed-bean-class>eredua.bean.Info</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>Hasiera.xhtml</display-name>
		<from-view-id>/Hasiera.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>query</from-outcome>
			<to-view-id>/QueryRides.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Hasiera.xhtml</display-name>
		<from-view-id>/Hasiera.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>create</from-outcome>
			<to-view-id>/CreateRide.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>CreateRide.xhtml</display-name>
		<from-view-id>/CreateRide.xhtml</from-view-id>
